<h1> Results: </h1>

<form action="/users_search" method="get">
  <div class="input-group input-inline search">
    <input class="form-input" type="text" name="query" placeholder=<%=I18n.t('search')%>>
     <button class="btn btn-primary input-group-btn"><%=I18n.t('search')%></button>
       </div>
      </form>

<% @users.each do |user| %>
    <li>
        <%= link_to user.email, edit_user_path(user.id) %>
        <%= user.role %>
        <h6> <%= I18n.t('registration_date') %>: <%= user.created_at.strftime('%B %d, %Y') %> </h6>
    </li>
<% end %>
